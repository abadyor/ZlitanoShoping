  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\CartComponent.razor" />
  </ItemGroup>

  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\CheckCodeForBasket.razor" />
